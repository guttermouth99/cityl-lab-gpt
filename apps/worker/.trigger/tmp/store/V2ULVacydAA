    var strToSnakeCase = require_str_to_snake_case();
    module.exports = /* @__PURE__ */ __name(function toSnakeCase(obj, ignored) {
      return convertKeys(obj, strToSnakeCase, ignored);
    }, "toSnakeCase");
  }
});

