{
  "extends": "@baito/typescript-config/base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@baito/trpc/*": ["./src/*"]
    },
    "outDir": "dist",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strictNullChecks": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
